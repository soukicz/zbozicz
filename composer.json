{
  "name": "soukicz/zbozicz",
  "description": "Zboží.cz conversion tracking",
  "minimum-stability": "stable",
  "license": "MIT",
  "keywords": [
    "zbozicz",
    "seznam",
    "zbozi",
    "seznamcz"
  ],
  "config": {
    "sort-packages": true
  },
  "authors": [
    {
      "name": "Petr Soukup",
      "email": "soukup@simplia.cz"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "ext-curl": "*",
    "ext-json": "*",
    "guzzlehttp/psr7": "~1.3",
    "psr/http-message": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0",
    "phpstan/phpstan": "~0.9|~0.10|~0.11"
  },
  "suggest": {
    "composer/ca-bundle": "Https certificates bundle"
  },
  "autoload": {
    "psr-4": {
      "Soukicz\\Zbozicz\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Soukicz\\TestZbozicz\\": "tests/"
    }
  }
}
